<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="body" format="string" />
    <attr name="startIcon" format="reference" />
    <attr name="endIcon" format="reference" />

    <declare-styleable name="MaskedTextInputEditText">
        <attr name="maskType" format="enum">
            <enum name="date" value="0" />
            <enum name="phone" value="1" />
            <enum name="ssn" value="2" />
            <enum name="currency" value="3" />
            <enum name="custom" value="4" />
            <enum name="static_text" value="5" />
            <enum name="credit_card" value="6" />
            <enum name="date_mm_yyyy" value="7" />
            <enum name="iban" value="8" />
            <enum name="tc_id" value="9" />
        </attr>
        <attr name="decimalSeparator" format="string" />
        <attr name="groupingSeparator" format="string" />
        <attr name="maskPattern" format="string" />
        <attr name="returnPattern" format="string" />
        <attr name="currencyDecimalLimit" format="integer" />
        <attr name="currencyPrefix" format="string" />
        <attr name="currencySuffix" format="string" />
        <!-- TODO ask here to yasin  -->
        <attr name="android:title" />
        <attr name="body" />
        <attr name="startIcon" />
        <attr name="endIcon" />
        <attr name="primaryListItemTitleStyle" format="reference" />
        <attr name="primaryListItemBodyStyle" format="reference" />
        <attr name="primaryListItemStartIconStyle" format="reference" />
        <attr name="primaryListItemEndIconStyle" format="reference" />
        <attr name="primaryListItemTopGuidelineStyle" format="reference" />
        <attr name="primaryListItemBottomGuidelineStyle" format="reference" />
    </declare-styleable>

</resources>
